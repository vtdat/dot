# Mute Master
"/usr/bin/amixer -q sset Master toggle"
	XF86AudioMute

# Increase Volume
"/usr/bin/amixer -q sset Master 2+"
	XF86AudioRaiseVolume

# Lower Volume
"/usr/bin/amixer -q sset Master 2-"
	XF86AudioLowerVolume

# Increase Brightness
"xbacklight -inc 5"
  XF86MonBrightnessUp

# Lower Brighness
"xbacklight -inc -5"
  XF86MonBrightnessDown
